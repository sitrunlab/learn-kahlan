{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Spec\\": "spec/"
        },
        "files": [
          "src/procedural.php"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.5|^5.6"
    },
    "require-dev": {
        "kahlan/kahlan": "dev-dev",
        "kahlan/extra-matcher": "^1.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "suggest": {
        "ext-xdebug": "Xdebug extension to allow to get coverage report",
        "ext-phpdbg": "PHPDBG extension to allow to get coverage report"
    }
}
